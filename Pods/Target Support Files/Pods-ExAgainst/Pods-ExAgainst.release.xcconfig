EMBEDDED_CONTENT_CONTAINS_SWIFT = YES
ENABLE_BITCODE = NO
FRAMEWORK_SEARCH_PATHS = $(inherited) "$CONFIGURATION_BUILD_DIR/IHKeyboardAvoiding" "$CONFIGURATION_BUILD_DIR/LTMorphingLabel" "$CONFIGURATION_BUILD_DIR/M13ProgressSuite" "$CONFIGURATION_BUILD_DIR/RMSwipeTableViewCell" "$CONFIGURATION_BUILD_DIR/Riffle-iOS" "$CONFIGURATION_BUILD_DIR/Spring"
GCC_PREPROCESSOR_DEFINITIONS = $(inherited) COCOAPODS=1
LD_RUNPATH_SEARCH_PATHS = $(inherited) '@executable_path/Frameworks' '@loader_path/Frameworks'
OTHER_CFLAGS = $(inherited) -iquote "$CONFIGURATION_BUILD_DIR/IHKeyboardAvoiding/IHKeyboardAvoiding.framework/Headers" -iquote "$CONFIGURATION_BUILD_DIR/LTMorphingLabel/LTMorphingLabel.framework/Headers" -iquote "$CONFIGURATION_BUILD_DIR/M13ProgressSuite/M13ProgressSuite.framework/Headers" -iquote "$CONFIGURATION_BUILD_DIR/RMSwipeTableViewCell/RMSwipeTableViewCell.framework/Headers" -iquote "$CONFIGURATION_BUILD_DIR/Riffle-iOS/Riffle.framework/Headers" -iquote "$CONFIGURATION_BUILD_DIR/Spring/Spring.framework/Headers"
OTHER_LDFLAGS = $(inherited) -ObjC -framework "IHKeyboardAvoiding" -framework "LTMorphingLabel" -framework "M13ProgressSuite" -framework "RMSwipeTableViewCell" -framework "Riffle" -framework "Spring"
OTHER_SWIFT_FLAGS = $(inherited) "-D" "COCOAPODS"
PODS_ROOT = ${SRCROOT}/Pods
